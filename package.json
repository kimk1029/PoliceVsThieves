{
  "name": "copvsrobbers",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:stage": "cd android && ./gradlew installDebug -PPNT_STAGE=true -PPNT_API_BASE_URL=http://kimk1029.synology.me:9991",
    "android:stage:apk": "cd android && ./gradlew assembleDebug -PPNT_STAGE=true -PPNT_API_BASE_URL=http://kimk1029.synology.me:9991",
    "android:stage:apk:release": "cd android && ./gradlew assembleRelease -PPNT_STAGE=true -PPNT_API_BASE_URL=http://kimk1029.synology.me:9991",
    "ios": "react-native run-ios",
    "ios:stage": "react-native run-ios --scheme PoliceVsThievesStage --mode Stage",
    "ios:stage:clean": "xcrun simctl uninstall booted com.copvsrobbers 2>/dev/null || true && xcrun simctl uninstall booted com.copvsrobbers.PoliceVsThieves 2>/dev/null || true && xcrun simctl uninstall booted org.reactjs.native.example.PoliceVsThieves 2>/dev/null || true && cd ios && export LANG=en_US.UTF-8 && pod install && cd .. && react-native run-ios --scheme PoliceVsThievesStage --mode Stage",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@mj-studio/react-native-naver-map": "2.7.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "^5.1.2",
    "qr.js": "0.0.0",
    "react": "19.1.0",
    "react-native": "0.80.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-incall-manager": "^4.2.1",
    "react-native-keep-awake": "^4.0.0",
    "react-native-vision-camera": "^4.7.0",
    "react-native-webrtc": "^124.0.7",
    "react-native-webview": "^13.10.2",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^20.1.0",
    "@react-native-community/cli-platform-android": "^20.1.0",
    "@react-native-community/cli-platform-ios": "^20.1.0",
    "@react-native/babel-preset": "^0.80.0",
    "@react-native/eslint-config": "^0.80.0",
    "@react-native/metro-config": "^0.80.0",
    "@react-native/typescript-config": "^0.80.0",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
